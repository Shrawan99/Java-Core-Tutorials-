
public class Constructor_Overloading {

	public static void main(String[] args) {
		Youtube you=new Youtube();
		new Youtube("You Did It");
			}
}

class Youtube{
	
	Youtube(){
		System.out.print("I am First Constructor");
	}
	Youtube(String name){
		
		System.out.print("I am Second Constructor"+name);
	}
}
